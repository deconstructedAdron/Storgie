        //
           Login Screen
        .login-wrapper
            .login-container
                a(href='/')
                    img(width='100', height='30', src='images/logo-login@2x.png')
                form(action='index.html')
                    .form-group
                        input.form-control(placeholder='Username or Email', type='text')
                    .form-group
                        input.form-control(placeholder='Password', type='text')
                        input(type='submit', value='')
                    .form-options.clearfix
                        a.pull-right(href='>') Forgot password?
                        .text-left
                            label.checkbox
                                input(type='checkbox')
                                span Remember me
                .social-login.clearfix
                    a.btn.btn-primary.pull-left.facebook(href='/')
                        i.icon-facebook
                        | Facebook login
                    a.btn.btn-primary.pull-right.twitter(href='/')
                        i.icon-twitter
                        | Twitter login
